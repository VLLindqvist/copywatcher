{
  "env": {
    "browser": true,
    "node": false
  },
  "extends": ["plugin:react/recommended", "plugin:react/jsx-runtime"],
  "settings": {
    "import/resolver": {
      "alias": {
        "map": [["assets", "./packages/renderer/assets"]],
        "extensions": [".woff2"]
      }
    },
    "react": {
      "version": "detect"
    }
  },
  "parserOptions": {
    "parser": "@typescript-eslint/parser",
    "ecmaVersion": 12,
    "sourceType": "module"
  }
}
